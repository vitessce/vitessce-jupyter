{
  "name": "vitessce-jupyter",
  "version": "0.1.13",
  "description": "Jupyter widget facilitating interactive visualization of spatial single-cell data with Vitessce",
  "author": "Mark Keller",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/vitessce/vitessce-jupyter.git"
  },
  "keywords": [
    "jupyter",
    "widgets",
    "ipython",
    "ipywidgets",
    "jupyterlab-extension"
  ],
  "files": [
    "lib/**/*.js",
    "dist/*.js"
  ],
  "scripts": {
    "clean": "rimraf dist/ && rimraf ../vitessce/labextension/ && rimraf ../vitessce/nbextension",
    "build:nbextension:dev": "webpack --mode=development",
    "build:nbextension": "webpack --mode=production",
    "build": "npm run build:nbextension && npm run build:labextension",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "watch": "webpack --watch --mode=development",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@jupyterlab/builder": "^3.4.1",
    "babel-loader": "8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "css-loader": "^1.0.1",
    "pnp-webpack-plugin": "^1.6.4",
    "react-dev-utils": "^10.2.1",
    "resolve": "^1.17.0",
    "rimraf": "^3.0.2",
    "style-loader": "^0.23.1",
    "webpack": "^4.37.0",
    "webpack-cli": "^3.3.6"
  },
  "dependencies": {
    "@jupyter-widgets/base": "^1.1 || ^2 || ^3 || ^4",
    "lodash": "^4.17.4",
    "pubsub-js": "^1.9.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "vitessce": "1.1.20"
  },
  "jupyterlab": {
    "extension": "dist/labplugin",
    "outputDir": "../vitessce/labextension",
    "sharedPackages": {
      "@jupyter-widgets/base": {
        "bundled": false,
        "singleton": true
      }
    }
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  }
}
